{
  "name": "personal-website-v4",
  "version": "1.0.0",
  "author": {
    "name": "Jordan Lambrecht",
    "email": "me@jordanlambrecht.com",
    "url": "https://jordanlambrecht.com"
  },
  "description": "My personal website",
  "license": "MIT",
  "private": "true",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev_old": "cross-env NODE_OPTIONS=--no-deprecation next dev ",
    "dev": " next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:schema": "cross-env NODE_OPTIONS=--no-deprecation payload generate:db-schema",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:status": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
    "migrate:down": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:down",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "reinstall": "rm -rf node_modules && rm pnpm-lock.yaml && rm -rf .next && pnpm store prune && pnpm install",
    "wipe": "rm -rf node_modules && rm pnpm-lock.yaml && rm -rf .next && pnpm store prune"
  },
  "dependencies": {
    "@faceless-ui/modal": "3.0.0-beta.2",
    "@faceless-ui/scroll-info": "2.0.0",
    "@formkit/auto-animate": "^0.8.2",
    "@innovixx/payload-color-picker-field": "^2.0.1",
    "@payloadcms/db-postgres": "^3.35.1",
    "@payloadcms/next": "3.35.1",
    "@payloadcms/plugin-redirects": "^3.35.1",
    "@payloadcms/plugin-seo": "3.35.1",
    "@payloadcms/richtext-lexical": "3.35.1",
    "@radix-ui/react-slot": "^1.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.9.1",
    "graphql": "^16.10.0",
    "lucide-react": "^0.503.0",
    "monaco-editor": "^0.52.2",
    "next": "^15.3.1",
    "next-sitemap": "^4.2.3",
    "payload": "3.35.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.1",
    "tailwind-merge": "^3.2.0",
    "yet-another-react-lightbox": "^3.23.0",
    "yjs": "^13.6.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "resolutions": {
    "file-type": "^20.4.0"
  },
  "yarn": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver",
      "sharp"
    ]
  }
}
